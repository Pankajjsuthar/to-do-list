<%- include("header") -%>
<body>
    <div class="box" id="heading">
        <h1><%= kindOfDay %></h1>
    </div>
    
    <!-- abhi isme ejs marker add krdenge jiski value change ho rhi hogi-->
    <div class="box">
            <% for(var i = 0; i<newListItem.length; i++){ %>
                <div class="item">
                    <input type="checkbox">
                    <p><%=newListItem[i]%></p>
                </div>
            <% } %>

            <form action="/" method="post" class="form">
                <input type="text" name="newItem" placeholder="New Item" autocomplete="off">
                <button name="button" type="submit" class="button">+</button>
            </form>
    </div>
    

    
</body>

<!-- footer file included -->
<%- include("footer") -%>

</html>